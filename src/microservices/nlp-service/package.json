{
  "name": "@crewai/nlp-service",
  "version": "1.0.0",
  "description": "Dedicated NLP microservice with GroceryNLPQueue and 2-operation limit for Walmart Grocery Agent",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t crewai-nlp-service .",
    "docker:run": "docker run -p 3001:3001 crewai-nlp-service",
    "health-check": "curl -f http://localhost:3001/health || exit 1"
  },
  "keywords": [
    "nlp",
    "microservice",
    "grocery",
    "walmart",
    "ollama",
    "queue"
  ],
  "author": "CrewAI Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@trpc/server": "^10.45.0",
    "fastify": "^4.26.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1",
    "prometheus-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "tsc-alias": "^1.8.8",
    "vitest": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}