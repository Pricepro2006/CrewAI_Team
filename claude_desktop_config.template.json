{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/home/pricepro2006/CrewAI_Team"],
      "env": {}
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "brightdata": {
      "command": "npx",
      "args": ["-y", "@brightdata/mcp-server"],
      "env": {
        "BRIGHTDATA_API_TOKEN": "${BRIGHTDATA_API_TOKEN}"
      }
    },
    "sequential": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "microsoft-graph": {
      "command": "node",
      "args": ["./mcp-servers/microsoft-graph/dist/index.js"],
      "cwd": "/home/pricepro2006/CrewAI_Team",
      "env": {
        "MSGRAPH_CLIENT_ID": "${MSGRAPH_CLIENT_ID}",
        "MSGRAPH_TENANT_ID": "${MSGRAPH_TENANT_ID}",
        "MSGRAPH_CLIENT_SECRET": "${MSGRAPH_CLIENT_SECRET}"
      }
    }
  }
}
